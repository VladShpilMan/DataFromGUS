<Window x:Class="PicturesOfCSO.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PicturesOfCSO"
        mc:Ignorable="d"
        xmlns:view="clr-namespace:PicturesOfCSO.View"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid>
            <view:ListCSO Grid.Column="0" />
            <ContentControl Grid.Column="1" Content="{Binding CurrentWindow}"/>
        </Grid>
    </Grid>
</Window>
